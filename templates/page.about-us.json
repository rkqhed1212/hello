    "163947753124fd5ee5": {
      "type": "home-grid-banner",
      "blocks": {
        "16394775318160aee4-0": {
          "type": "content",
          "settings": {
            "block_image": "shopify:\/\/shop_images\/123_dffc102e-3277-47c8-bb58-fa11847141c4.jpg",
            "show_image": true,
            "block_title": "Shane Kim",
            "block_sub_title": "Tea Sommelier",
            "block_description": "During a transformative shift from my career as an IT Project Manager for a global company, I made a life-changing decision: to create KTea Shop with my wife, Nicole. Growing up in South Korea, I was profoundly influenced by the tradition of tea ceremonies, where sharing tea fosters deep, meaningful connections. As the owner and a devoted Tea Sommelier, I felt a calling to bring this beautiful tradition to life through our shop, inspiring others to experience the powerful bonds that can be forged over a simple cup of tea.",
            "block_button_text": "Experience tea",
            "block_button_link": "https:\/\/kteashop.com\/pages\/service-1",
            "block_text_align": "text-start",
            "content_position": "center-left",
            "reverse_column": false,
            "block_banner_bg": "rgba(0,0,0,0)",
            "block_content_box_bg": "rgba(0,0,0,0)",
